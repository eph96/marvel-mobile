{
  "expo": {
    "scheme": "acme",
    "owner":"josuecr",
    "name": "mi",
    "slug": "mi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.test.mi"
    },
    "android": {
      "package": "com.test.mi",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"

    },
    "extra": {
      "eas": {
        "projectId": "1a97e4f3-4cdd-4be1-9443-79a7c5445087"
      }
    }

  }
}
